{
  "main": "main.js",
  "scripts": {
    "start": "node_modules/.bin/electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "sayooj.whatsapp.deskapp",
    "productName": "WhatsApp App by Sayooj",
    "target": "NSIS",
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "devDependencies": {
    "electron": "^35.1.2",
    "electron-builder": "^26.0.12"
  },
  "build": {
  "appId": "sayooj.whatsapp.deskapp",
  "linux": {
    "target": ["AppImage", "deb", "rpm", "snap", "tar.gz"],
    "category": "Network",
    "icon": "assets/whatsapp-icon.png"
    }
  },
"scripts": {
  "start": "electron .",
  "pack": "electron-builder --dir",
  "dist": "electron-builder --linux"
},
  "name": "whatsapp_app_sayooj",
  "version": "1.0.0",
  "description": "WhatsApp client built with Electron by Sayooj",
  "repository": "https://github.com/sayooxx/Whatsapp_App-Electron",
  "author": "sayooxx <metech87@gmail.com>",
  "license": "MIT"
}

